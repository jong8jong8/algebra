   "source": [
    "%matplotlib widget\n",
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "x = np.arange(-6, 6, 0.01)\n",
    "y = x**2 + 2*x\n",
    "y3 = [3]*len(x)\n",
    "plt.style.use('seaborn')\n",
    "fig, ax = plt.subplots()\n",
    "ax.plot(x, y, x, y3)\n",
    "ax.scatter(1, 3)\n",
    "ax.scatter(-3, 3)\n",
    "ax.scatter(4, 24)\n",
    "plt.xlim(-6, 6)\n",
    "plt.ylim(-6, 36)"
   ]
  },
