   "source": [
    "from sympy import symbols, Eq, linsolve"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 44,
   "id": "12970a1e-992f-423f-aabe-2da925558da1",
   "metadata": {},
   "outputs": [],
   "source": [
    "x, y = symbols('x, y')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 50,
   "id": "43e0ebab-0c1e-44cf-b9ee-ddc0eb9f9d6d",
   "metadata": {},
   "outputs": [],
   "source": [
    "eq1 = Eq(x + 3*y, 2)\n",
    "eq2 = Eq(3*x + 9*y, 6)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 51,
   "id": "bf082b5c-11c5-450c-a9cc-5b38b34739f3",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/latex": [
       "$\\displaystyle \\left\\{\\left( 2 - 3 y, \\  y\\right)\\right\\}$"
      ],
      "text/plain": [
       "FiniteSet((2 - 3*y, y))"
      ]
     },
     "execution_count": 51,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "linsolve([eq1, eq2], (x, y))"
   ]
  },
