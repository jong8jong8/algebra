   "source": [
    "# Figure 5\n",
    "%matplotlib widget\n",
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "x = np.arange(-7, 7, 0.01)\n",
    "y1 = 3*x - 1\n",
    "y2 = (-1/3)*x - 2\n",
    "\n",
    "plt.style.use('seaborn')\n",
    "fig, ax = plt.subplots()\n",
    "ax.plot(x, y1, x, y2)\n",
    "ax.a('equal', 'box')"
   ]
  },
